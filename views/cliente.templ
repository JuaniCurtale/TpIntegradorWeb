package views

import "tpIntegradorSaideCurtale/db/sqlc"

templ ClientesPage(clientes []db.Cliente) {
    @Layout("Clientes", 
        <section>
            <h2>Lista de Clientes</h2>
            @ClienteList(clientes)
            <h3>Agregar nuevo cliente</h3>
            @ClienteForm()
        </section>
    )
}
